<li id="newWidget" data-rank="<%= rank %>">

  <div class="widget-form">

    <div class="form-header">
      <h4>
        What would you like to add here?
        <a href="#" class='cancel close'>
          <span class='glyphicon glyphicon-remove'></span>
        </a>
      </h4>
    </div>

    <div class="form-fields">
      <a href="#"
        class='btn btn-default btn-sm btn-add-widget'
        data-type="text">
        <span class='glyphicon glyphicon-font'></span><br>
        Add Text
      </a>

      <a href='#'
        class='btn btn-default btn-sm btn-add-widget'
        data-type="contact">
        <span class='glyphicon glyphicon-envelope'></span><br>

        Add Contact
      </a>

      <a href='#'
        class='btn btn-default btn-sm btn-add-widget'
        data-type="social">
        <span class='glyphicon glyphicon-comment'></span><br>

        Add Social
      </a>

      <a href='#'
        class='btn btn-default btn-sm btn-add-widget'
        data-type="services">
        <span class='glyphicon glyphicon-th-list'></span><br>
        Add Services
      </a>

      <a href='#'
        class='btn btn-default btn-sm btn-add-widget'
        data-type="image">
        <span class='glyphicon glyphicon-picture'></span><br>
        Add Image
      </a>

      <a href='#'
        class='btn btn-default btn-sm btn-add-widget'
        data-type="button">
        <span class='glyphicon glyphicon-unchecked'></span><br>
        Add Button
      </a>


    </div>

  </div>
</li>